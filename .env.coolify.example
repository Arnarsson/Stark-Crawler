# STARK Crawler Environment Configuration for Coolify
# Copy this file to .env and update with your actual values

# === Supabase Configuration ===
SUPABASE_URL=your_supabase_project_url
SUPABASE_KEY=your_supabase_anon_key
SUPABASE_SERVICE_KEY=your_supabase_service_role_key

# === Crawler Configuration ===
CRAWLER_CONCURRENCY=2
CRAWLER_HEADLESS=true
LOG_LEVEL=info

# === n8n Configuration ===
N8N_USER=admin
N8N_PASSWORD=your_secure_n8n_password
N8N_WEBHOOK_URL=https://your-n8n-domain.com
N8N_DB_PASSWORD=your_n8n_db_password

# === PostgreSQL Configuration ===
DB_PASSWORD=your_postgres_password

# === PgAdmin Configuration (Optional) ===
PGADMIN_EMAIL=admin@your-domain.com
PGADMIN_PASSWORD=your_pgladmin_password

# === Optional: Direct PostgreSQL Connection ===
# Only needed if you want to use direct PostgreSQL instead of Supabase
DB_HOST=your_postgres_host
DB_PORT=5432
DB_NAME=stark_products
DB_USER=postgres